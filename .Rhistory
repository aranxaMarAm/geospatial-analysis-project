qtm(joined_sf, fill="lmI_sign")
tm_shape(joined_sf) +
tm_polygons(
fill = "lmI_sign",
fill.legend = tm_legend(title = "spatial autocorrelation"),
fill.scale=tm_scale(
breaks = c(min(joined_sf$lmI_sign, na.rm=T), 0, max(joined_sf$lmI_sign, na.rm=T)),
